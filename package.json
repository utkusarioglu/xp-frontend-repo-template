{
  "name": "xp-frontend-repo-template.templates.utkusarioglu.com",
  "packageManager": "yarn@4.0.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:no-telemetry": "turbo run dev:no-telemetry",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "rm -rf node_modules **/**/node_modules apps/android/android/app/build apps/web/.next"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "turbo": "^1.11.2"
  }
}
